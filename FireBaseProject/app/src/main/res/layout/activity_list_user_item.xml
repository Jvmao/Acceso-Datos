<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/sky1hdjpg"
    tools:context="com.josevicente.firebaseproject.ItemControl.ListUserItemActivity">

    <ListView
        android:id="@+id/listUserItem"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:divider="@android:color/holo_orange_dark"
        android:dividerHeight="@dimen/cardview_default_radius"
        android:footerDividersEnabled="true"
        android:headerDividersEnabled="true"
        android:scrollbars="vertical" />
</RelativeLayout>
